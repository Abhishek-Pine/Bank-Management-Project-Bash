#! /bin/bash

clear
echo -e "\n\tWelcome to Customer Menu\n"
echo -e "\n1. Savings Account \n2. Current Account \n3. Exit \n"
echo -n "Enter Your Choice : "
read ch

case $ch in
1)
ac='Saving'
./custmenu $ac
;;

2)
ac='Current'
./custmenu $ac
;;

*)
echo -e "\nOps!!!! Invalid Input \n\n"
;;
esac
done
